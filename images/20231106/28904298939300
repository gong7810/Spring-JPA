[an9]
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <script>
      let array = ["red", "green", "blue"];
      array.forEach((color) => {
        let btn = document.createElement("button");
        btn.innerText = color;
        btn.addEventListener("mouseover", f);
        btn.addEventListener("mouseleave", f2);
        document.body.appendChild(btn);
      });
      function f() {
        this.style.backgroundColor = this.innerText;
      }
      function f2() {
        this.style.backgroundColor = "";
      }
    </script>
  </body>
</html>



[an10-1 토클없이]
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <script>
      let div1 = document.createElement("div");
      let div2 = document.createElement("div");
      div1.innerText = "소스코드숨기기";
      div1.addEventListener("click", f);
      function f() {
        if (this.innerText == "소스코드숨기기") {
          this.innerText = "소스코드보여주기";
        } else if (this.innerText == "소스코드보여주기") {
          this.innerText = "소스코드숨기기";
        }
      }
      document.body.appendChild(div1);
    </script>
  </body>
</html>


[an10-2 토클사용]
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <style>
      .hidden {
        display: none;
      }
    </style>
  </head>
  <body>
    <script>
      const div1 = document.createElement("div");
      const div2 = document.createElement("div");
      div1.innerText = "소스코드숨기기";
      div2.innerText = "소스코드보여주기";
      div2.setAttribute("class", "hidden");
      div1.addEventListener("click", function () {
        div1.classList.toggle("hidden");
        div2.classList.toggle("hidden");
      });
      div2.addEventListener("click", function () {
        div1.classList.toggle("hidden");
        div2.classList.toggle("hidden");
      });
      document.body.appendChild(div1);
      document.body.appendChild(div2);
    </script>
  </body>
</html>






































