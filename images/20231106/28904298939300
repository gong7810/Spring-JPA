[an7]
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <script>
      let dataset = new Dataset("aa", 11, "가좌동", "010-1111-1111");
      function Dataset(id, pw, addr, tel) {
        this.id = id;
        this.pw = pw;
        this.addr = addr;
        this.tel = tel;
        this.list = [
          { id: this.id, pw: this.pw, addr: this.addr, tel: this.tel },
          { id: this.id, pw: this.pw, addr: this.addr, tel: this.tel },
          { id: this.id, pw: this.pw, addr: this.addr, tel: this.tel },
          { id: this.id, pw: this.pw, addr: this.addr, tel: this.tel },
        ];
      }
      console.log(dataset.list[0].id); // aa
      console.log(dataset.list[1].pw); // 11
      console.log(dataset.list[2].addr); // 가좌동
      console.log(dataset.list[3].tel); // 010-1111-1111
    </script>
  </body>
</html>


